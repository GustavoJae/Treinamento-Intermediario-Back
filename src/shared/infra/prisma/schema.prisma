datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         String   @id @default(uuid())
  name       String   @unique
  email      String   @unique
  password   String
  avatar     String?
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
  cpf        String   @unique
  telephone  String
  birthDate  DateTime

  @@map("users")
}